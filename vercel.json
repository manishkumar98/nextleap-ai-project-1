{
  "version": 2,
  "name": "ai-restaurant-scout",
  "rewrites": [
    {
      "source": "/(locations|cuisines|recommendations|health)",
      "destination": "/api/index.py"
    },
    {
      "source": "/static/(.*)",
      "destination": "/frontend/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/frontend/index.html"
    }
  ]
}