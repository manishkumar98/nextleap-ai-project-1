{
  "version": 2,
  "name": "ai-restaurant-scout",
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "frontend/$1"
    },
    {
      "src": "/locations",
      "dest": "api/index.py"
    },
    {
      "src": "/cuisines",
      "dest": "api/index.py"
    },
    {
      "src": "/recommendations",
      "dest": "api/index.py"
    },
    {
      "src": "/health",
      "dest": "api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/index.html"
    }
  ]
}